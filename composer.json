{
    "name": "repat/immutability",
    "description": "A simple Laravel package based on davidmpeace/immutability that allows you to enforce immutable attributes on Eloquent models.",
    "keywords": ["laravel","eloquent","immutable","attributes","orm","automatic","immutability","davidmpeace","repat"],
    "license": "MIT",
    "minimum-stability": "stable",
    "version":"0.3",
    "authors": [
        {
            "name": "David Peace",
            "email": "peaceman@gmail.com"
        },
        {
            "name": "repat",
            "email": "repat@repat.de"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "4.*|5.*"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "~4.0",
        "illuminate/database": "4.*|5.*"
    },
    "autoload": {
        "psr-4": {
            "Eloquent\\Attributes\\": "src/"
        }
    }
}
